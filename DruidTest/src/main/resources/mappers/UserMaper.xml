<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youfan.mapper.UserMapper">

    <select id="getUserInfo" parameterType = "com.youfan.vo.UserInfoRequest"
            resultType = "com.youfan.vo.UserInfoResponse">
        SELECT * FROM user WHERE name = #{username} AND passwordencrypt = #{password}
    </select>

</mapper>
